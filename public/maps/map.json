{
  "$schema": "https://shadcn-vue.com/schema/registry-item.json",
  "name": "map",
  "type": "registry:ui",
  "title": "Map",
  "description": "A MapLibre-powered map component with markers, popups, tooltips, routes, and controls.",
  "dependencies": ["maplibre-gl", "lucide-vue-next"],
  "registryDependencies": [],
  "files": [
    {
      "path": "registry/map/index.ts",
      "type": "registry:utils",
      "target": "components/ui/map/index.ts"
    },
    {
      "path": "registry/map/Map.vue",
      "type": "registry:component",
      "target": "components/ui/map/Map.vue"
    },
    {
      "path": "registry/map/MapMarker.vue",
      "type": "registry:component",
      "target": "components/ui/map/MapMarker.vue"
    },
    {
      "path": "registry/map/MarkerContent.vue",
      "type": "registry:component",
      "target": "components/ui/map/MarkerContent.vue"
    },
    {
      "path": "registry/map/MarkerPopup.vue",
      "type": "registry:component",
      "target": "components/ui/map/MarkerPopup.vue"
    },
    {
      "path": "registry/map/MarkerTooltip.vue",
      "type": "registry:component",
      "target": "components/ui/map/MarkerTooltip.vue"
    },
    {
      "path": "registry/map/MarkerLabel.vue",
      "type": "registry:component",
      "target": "components/ui/map/MarkerLabel.vue"
    },
    {
      "path": "registry/map/MapPopup.vue",
      "type": "registry:component",
      "target": "components/ui/map/MapPopup.vue"
    },
    {
      "path": "registry/map/MapControls.vue",
      "type": "registry:component",
      "target": "components/ui/map/MapControls.vue"
    },
    {
      "path": "registry/map/MapRoute.vue",
      "type": "registry:component",
      "target": "components/ui/map/MapRoute.vue"
    },
    {
      "path": "registry/map/MapClusterLayer.vue",
      "type": "registry:component",
      "target": "components/ui/map/MapClusterLayer.vue"
    },
    {
      "path": "registry/map/constants.ts",
      "type": "registry:utils",
      "target": "components/ui/map/constants.ts"
    },
    {
      "path": "registry/map/composables/useMap.ts",
      "type": "registry:hook",
      "target": "components/ui/map/composables/useMap.ts"
    },
    {
      "path": "registry/map/composables/useMarker.ts",
      "type": "registry:hook",
      "target": "components/ui/map/composables/useMarker.ts"
    },
    {
      "path": "registry/map/composables/useResolvedTheme.ts",
      "type": "registry:hook",
      "target": "components/ui/map/composables/useResolvedTheme.ts"
    }
  ],
  "css": {
    "@layer base": {
      ".maplibregl-popup-content": {
         "@apply bg-transparent! shadow-none! p-0! rounded-none!": {}
      },
      ".maplibregl-popup-tip": {
        "@apply hidden!": {}
      }
    }
  }
}
