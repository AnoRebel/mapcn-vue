  "css": {
    "@layer base": {
      ".maplibregl-popup-content": {
        "background-color": "transparent",
        "box-shadow": "none",
        "padding": "0",
        "border-radius": "0"
      },
      ".maplibregl-popup-tip": {
        "display": "none"
